generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id          String   @id @default(uuid())
  githubId    String   @unique
  accessToken String
  username    String?
  avatarUrl   String?
  displayName String?
  profileUrl  String?
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt
}
